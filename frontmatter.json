{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "notes",
      "pageBundle": true,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Thumbnail",
          "name": "thumbnail",
          "type": "image",
          "default": "thumbnail.png"
        },
        {
          "title": "Cover",
          "name": "cover",
          "type": "image",
          "default": "cover.png"
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image",
          "default": "thumbnail.png"
        },
        {
          "title": "Weight",
          "name": "weight",
          "type": "number"
        }
      ]
    }
  ],
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]/content/notes",
      "title": "notes"
    },
    {
      "path": "[[workspace]]/content/experience",
      "title": "experience"
    },
    {
      "path": "[[workspace]]/content/about",
      "title": "about"
    },
    {
      "title": "content",
      "path": "[[workspace]]/WSL.LOCALHOST/UBUNTU-24.04/home/austin/dev/austinmaddison.github.io/content"
    }
  ],
  "frontMatter.content.publicFolder": "static",
  "frontMatter.framework.id": "hugo",
  "frontMatter.preview.host": "http://localhost:1313"
}